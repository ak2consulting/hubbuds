!!!
%html
	%head
		= javascript_include_tag :defaults, "http://use.typekit.com/jxe7poo.js"
		= stylesheet_link_tag 'compiled/screen.css', :media => 'screen, projection'
		= stylesheet_link_tag 'compiled/print.css', :media => 'print'
		/[if lt IE 8]
			= stylesheet_link_tag 'compiled/ie.css', :media => 'screen, projection'
		:javascript
			try{Typekit.load();}catch(e){}
	%body.bp
		#container
			#header
				%table.no-padding
					%tr
						%td{:width=>'200px'}
							= image_tag('logo.png')
						%td.align-right.top
							joelmahoney &nbsp;|&nbsp; logout
			#page
				.status-bar
					%ul.horizontal-list
						%li 37%
						%li 37%
						%li 37%
						%li 37%
				= yield